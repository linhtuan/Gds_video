<script id="categoryDetailTemplate" type="text/x-jquery-tmpl">
    <table class="table table-striped table-bordered table-hover dataTable dtr-inline" id="categoryDetailTemplate_Table" role="grid" aria-describedby="DataTables_Table_0_info">
        <thead>
            <tr role="row">
                <th class="sorting_asc" tabindex="0" aria-controls="categoryDetailTemplate_Table" rowspan="1" colspan="1" aria-sort="ascending" style="width: 189px;">Tên bài</th>
                <th class="sorting_asc" tabindex="0" aria-controls="categoryDetailTemplate_Table" rowspan="1" colspan="1" aria-sort="ascending" style="width: 189px;">Nội dung</th>
                <th class="sorting_asc" tabindex="0" aria-controls="categoryDetailTemplate_Table" rowspan="1" colspan="1" aria-sort="ascending" style="width: 189px;">Tên file</th>
                <th class="sorting" tabindex="0" aria-controls="categoryDetailTemplate_Table" rowspan="1" colspan="1" style="width: 162px;">Ngày tạo</th>
                <th tabindex="0" aria-controls="categoryDetailTemplate_Table" rowspan="1" colspan="1" style="width: 115px;"></th>
            </tr>
        </thead>
        <tbody>
            {{each Result}}
            <tr class="odd" role="row" id="${$value.CategoryDetailId}">
                <td class="categorty-detail">${$value.CategoryDetailName}</td>
                <td class="content">${$value.Content}</td>
                <td class="fileName">${$value.FileName}</td>
                <td class="create-date">${$value.UpdatedDate}</td>
                <td class="actions">
                    <a class="btn btn-sm btn-success btn-rounded show-detail-box" onclick="bindingCategoryDetailFrom(this, 'edit')" data-id="${$value.CategoryDetailId}" data-toggle="modal" href="#add-new-category-detail">Edit</a>
                    <a class="btn btn-sm btn-danger btn-rounded" onclick="deleteCategoryDetail(this)" data-id="${$value.CategoryDetailId}" href="#">Delete</a>
                </td>
            </tr>
            {{/each}}
        </tbody>
    </table>
</script>


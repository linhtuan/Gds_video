<script id="categoryTypeChildrenTemplate" type="text/x-jquery-tmpl">
    <table class="table table-striped table-bordered table-hover dataTables-example dataTable dtr-inline" id="categoryTypeChildren_Table" role="grid" aria-describedby="DataTables_Table_0_info">
        <thead>
            <tr role="row">
                <th class="sorting_asc" tabindex="0" aria-controls="categoryTypeChildren_Table" rowspan="1" colspan="1" aria-sort="ascending" style="width: 189px;">Tên gói</th>
                <th class="sorting_asc" tabindex="0" aria-controls="categoryTypeChildren_Table" rowspan="1" colspan="1" aria-sort="ascending" style="width: 189px;">Tên khóa học kèm theo</th>
                <th class="sorting" tabindex="0" aria-controls="categoryTypeChildren_Table" rowspan="1" colspan="1" style="width: 253px;">Nội dung</th>
                <th class="sorting" tabindex="0" aria-controls="categoryTypeChildren_Table" rowspan="1" colspan="1" style="width: 229px;">Giá tiền</th>
                <th class="sorting" tabindex="0" aria-controls="categoryTypeChildren_Table" rowspan="1" colspan="1" style="width: 229px;">Số thứ tự</th>
                <th class="sorting" tabindex="0" aria-controls="categoryTypeChildren_Table" rowspan="1" colspan="1" style="width: 162px;">Ngày tạo</th>
                <th tabindex="0" aria-controls="categoryTypeChildren_Table" rowspan="1" colspan="1" style="width: 115px;"></th>
            </tr>
        </thead>
        <tbody>
            {{each Result}}
            <tr class="gradeA odd" role="row" id="${$value.ChildrenId}">
                <td class="parent-name" data-parentid="${$value.ParentId}">${$value.ParentName}</td>
                <td class="category-type-name" data-childrenid="${$value.ChildrenId}">${$value.ChildrenName}</td>
                <td class="content-detail">{{html $value.Content}}</td>
                <td class="price" data-priceid="${$value.PriceId}">${$value.Price}</td>
                <td class="children-index">${$value.ChildrenIndex}</td>
                <td class="center">${$value.DateTime}</td>
                <td class="center">
                    <a class="btn btn-sm btn-success btn-rounded" href="categorydetail?categoryTypeId=${$value.ChildrenId}">Bài học</a>
                    <a class="btn btn-sm btn-success btn-rounded" onclick="bindingCategoryTypeDetail(this)" data-type="children" data-id="${$value.ChildrenId}" data-toggle="modal" href="#add-new-category-type-children">Edit</a>
                    <a class="btn btn-sm btn-danger btn-rounded" onclick="deleteCategoryType(this)" data-type="children" data-id="${$value.ChildrenId}" href="#">Delete</a>
                </td>
            </tr>
            {{/each}}
        </tbody>
    </table>
</script>
